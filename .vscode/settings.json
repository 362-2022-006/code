{
    "cortex-debug.variableUseNaturalFormat": true,
    "files.associations": {
        "*.h": "cpp",
        "*.s": "arm",
        "sstream": "c",
        "__bit_reference": "c",
        "__string": "c",
        "cstring": "c",
        "random": "c",
        "queue": "c",
        "*.tcc": "c"
    },
    "C_Cpp.clang_format_style": "{ BasedOnStyle: LLVM, UseTab: Never, IndentWidth: 4, ColumnLimit: 100, }",
    "disasexpl.associations": {
        "**/*.c": "${workspaceFolder}/build/intermediates/${fileBasenameNoExtension}.s"
    }
}
